{
  "name": "plaisio/phing-extensions",
  "description": "PhpPlaisio: Phing Extensions",
  "keywords": [
    "PhpPlaisio",
    "Phing Extensions"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-zlib": "*",
    "phing/phing": "3.0.x-dev",
    "plaisio/helper-url": "^1.0",
    "plaisio/kernel": "^1.0",
    "setbased/exception": "^2.0",
    "setbased/helper-cast": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0",
    "plaisio/page-core": "^1.0",
    "setbased/helper-program-execution": "^1.0",
    "setbased/phing-extensions": "dev-master"
  },
  "autoload": {
    "classmap": ["src"]
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Phing\\Task\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
