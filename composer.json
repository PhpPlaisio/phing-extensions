{
  "name": "setbased/abc-phing",
  "description": "ABC Framework: Phing Extensions for SetBased",
  "type": "library",
  "keyword": [
    "ABC-Framework",
    "phing",
    "extensions"
  ],
  "license": "MIT",
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^6.0",
    "setbased/helper-program-execution": "^1.0",
    "setbased/phing-extensions": "^2.0"
  },
  "require": {
    "php": ">=7.0.0",
    "setbased/abc": "^0.10.0",
    "setbased/abc-url": "^1.0.0",
    "setbased/exception": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Phing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Phing\\Test\\": "test"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
