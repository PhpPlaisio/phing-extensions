{
  "name": "plaisio/phing-extensions",
  "description": "PhpPlaisio: Phing Extensions",
  "type": "library",
  "keywords": [
    "PhpPlaisio",
    "Phing Extensions"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "plaisio/helper-url": "^1.0.0",
    "plaisio/kernel": "^1.0.0",
    "setbased/exception": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "setbased/helper-program-execution": "^1.0.0",
    "setbased/phing-extensions": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Phing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Phing\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
