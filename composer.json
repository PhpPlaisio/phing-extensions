{
  "name": "plaisio/phing-extensions",
  "description": "PhpPlaisio: Phing Extensions",
  "keywords": [
    "PhpPlaisio",
    "Phing Extensions"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-zlib": "*",
    "phing/phing": "3.0.x-dev",
    "plaisio/helper-url": "^1.1",
    "plaisio/kernel": "^3.0",
    "plaisio/web-assets": "^2.0",
    "setbased/exception": "^2.1",
    "setbased/helper-cast": "^2.1",
    "setbased/helper-program-execution": "^1.1",
    "setbased/php-stratum-sqlite-pdo": "^1.3",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "plaisio/console": "^2.2",
    "plaisio/page-core": "^1.2",
    "setbased/phing-extensions": "dev-master",
    "setbased/php-stratum": "^6.0"
  },
  "autoload": {
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Phing\\Task\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
